{
    "name": "fftcg-collection-api",
    "version": "0.1.0",
    "description": "",
    "main": "src/app.ts",
    "scripts": {
        "start": "node build/app.js",
        "build": "tsc",
        "check": "tsc --noemit",
        "watch": "nodemon",
        "eslint": "eslint --ext .ts ./src ./test",
        "eslint:fix": "eslint --ext .ts --fix ./src ./test",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:generate": "npm run typeorm -- migration:generate -n",
        "fixture": "fixtures ./fixtures -c ./ormconfig.js --require=ts-node/register",
        "fixture:test": "fixtures ./fixtures -cn test -s -c ./ormconfig.js --require=ts-node/register",
        "test": "npm run test:unit && npm run test:func",
        "test:unit": "NODE_ENV=test npx mocha -r ts-node/register --exclude test/controllers/**/*.ts test/**/*.ts",
        "test:func": "NODE_ENV=test npx mocha -r ts-node/register test/controllers/**/*.ts",
        "test:func:cov": "NODE_ENV=test npx nyc mocha -r ts-node/register test/controllers/**/*.ts",
        "card": "npx ts-node src/commands/cardsLoader.ts",
        "card:regen": "npm run card -- --regen-file",
        "card:test": "NODE_ENV=test npm run card"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:micriber/fftcg-collection-api.git"
    },
    "author": "Micriber",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.1",
        "google-auth-library": "^6.0.6",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.3.0",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.25",
        "typeorm-pagination": "^2.0.1",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/chai": "^4.2.12",
        "@types/express": "^4.17.7",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mocha": "^8.0.2",
        "@types/node": "^14.0.27",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/yamljs": "^0.2.31",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^7.7.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "mocha": "^8.1.1",
        "nodemon": "^2.0.4",
        "nyc": "^15.1.0",
        "ts-node": "^8.10.2",
        "typeorm-fixtures-cli": "^1.7.1",
        "typescript": "^3.9.7"
    }
}
